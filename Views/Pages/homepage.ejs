    <!-- ================ trending product section start ================= -->  
    <section class="section-margin calc-60px">
      <div class="container text-center">
        <div class="section-intro pb-60px text-center">
          <p>Popular Item in the market</p>
          <h2>Featured <span class="section-intro__style">Product</span></h2>
        </div>
        <div class="row">
          <!-- start -->
         <% output.forEach(element => { %>
          <div class="col-md-6 col-lg-4 col-xl-3">
            <div class="card text-center card-product">
              <div class="card-product__img" >
                <img class="card-img" src="images/<%= element.image %> " alt="">
                <ul class="card-product__imgOverlay">
                  <li> <h4 class="card-product__title"><a class="text-dark" href="/prod_view/<%= element.id %>">View Product</a></h4></li>
                </ul>
              </div>
              <div class="card-body">
                <h4 class="card-product__title"><a href="/prod_view/<%= element.id %>"><%= element.title %></a></h4>
                <p class="card-product__price">&#8369;<%= element.price %></p>
                <p><%= element.description %></p>
              </div>
            </div>
          </div>
         <% }) %>
          <!-- end -->
        </div>
      </div>
    </section>
    <!-- ================ trending product section end ================= -->  


    <!-- ================ offer section start ================= --> 
    <section class="offer" id="parallax-1" data-anchor-target="#parallax-1" data-300-top="background-position: 20px 30px" data-top-bottom="background-position: 0 20px">
      <div class="container">
        <div class="row">
